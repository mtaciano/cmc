{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc build active file",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${fileDirname}/cmin.tab.o",
        "${fileDirname}/lex.yy.o",
        "${fileDirname}/main.o",
        "${fileDirname}/util.o",
        "${fileDirname}/symtab.o",
        "${fileDirname}/analyze.o",
        "-o",
        "${fileDirname}/compiler"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
