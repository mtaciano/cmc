int fib(int x, int y, int n) {
    int i;
    int z;

    z = x + y;

    i = 3;
    while (i <= n) {
        output(z);
        x = y;
        y = z;
        z = x + y;

        i = i + 1;
    }

    return z;
}

void main(void)
{
	int num;
    int res;

    num = input();
    res = fib(0, 1, num);

    output(res);
}
