/* Um programa para calcular o mdc segundo o algoritmo de Euclides */
/* Linha 13: parâmetro nào pode ser void */

int gcd(int u, int v) {
  if (v==0) return u;
  else return gcd(v, u-u/v*v);
  /* u-u/v*v == u mod v */
}

void main(void)
{
  int x; int y;
  void z;
  x = input(); y = input();
  output(gcd(x,y));
}
