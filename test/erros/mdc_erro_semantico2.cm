/* Um programa para calcular o mdc segundo o algoritmo de Euclides */
/* Linha 13: vari√°vel declarada duas vezes */

int gcd(int u, int v) {
  if (v==0) return u;
  else return gcd(v, u-u/v*v);
  /* u-u/v*v == u mod v */
}

void main(void)
{
  int x; int y;
  int x;
  x = input(); y = input();
  output(gcd(x,y));
}
