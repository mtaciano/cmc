int fat(int n) {
    int num;
    int i;

    i = n;

    num = 1;
    while (i >= 1) {
        num = num * i;
        i = i - 1;
    }

    return num;
}

void main(void) {
    int i;
    int x;
    int y;
    int vec[10];

    i = 0;
    x = input();
    y = fat(x);

    while (i < y) {
        if (i == x) {
            vec[i] = y;
            i = i + (2 * x);
        } else {
            i = i + x;
        }
    }

    output(vec[x]);
}
