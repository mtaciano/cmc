/* Um programa para calcular o fatorial de um nÃºmero
 * e.g. n = 5 -> 5! = 5*4*3*2*1 = 120
 */

int factorial(int n) {
    int num;
    int i;
    i = n;
    num = 1;

    while (i >= 1) {
        num = num * i;
        i = i - 1;
    }

    return num;
}

void main(void) {
    int x;
    x = input();

    output(factorial(x));
}
